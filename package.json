{
  "name": "@shapeshiftoss/web",
  "version": "1.27.0",
  "private": true,
  "engines": {
    "node": "22.x"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ],
    "version": "independent"
  },
  "scripts": {
    "dev": "pnpm exec concurrently \"pnpm run dev:packages\" \"pnpm run dev:web --mode=${MODE:-development}\"",
    "preview:prod": "MODE=production pnpm run build:web && vite preview",
    "preview:dev": "MODE=development pnpm run build:web && vite preview",
    "preview:private": "MODE=private pnpm run build:web && vite preview",
    "analyze": "ANALYZE=true pnpm run build:web",
    "bump": "pnpm update -i --latest --filter \"@shapeshiftoss/*\"",
    "build:web": "pnpm run build:packages && NODE_OPTIONS=--max-old-space-size=16000 vite build --mode=${MODE:-production} && ./scripts/sha256sums.sh && ./scripts/verifyViteHashes.sh && tsx ./scripts/writeBuildMetadata.ts",
    "build:packages": "pnpm run clean:packages && pnpm --filter @shapeshiftoss/unchained-client generate && pnpm exec tsc --build tsconfig.packages.json && pnpm run postbuild:packages",
    "postbuild:packages": "pnpm -r --filter=\"!@shapeshiftoss/web\" postbuild",
    "clean": "pnpm run clean:packages && pnpm run clean:web && rm -rf node_modules coverage",
    "clean:web": "rm -rf build",
    "clean:packages": "pnpm -r --filter=\"!@shapeshiftoss/web\" clean",
    "codemod:clear-assets-migration": "jscodeshift -t src/state/migrations/transformClearAssets.ts --parser=ts --extensions=ts src/state/migrations/index.ts",
    "deadcode": "pnpm exec ts-prune",
    "dev:web": "NODE_OPTIONS=--max-old-space-size=8192 vite",
    "dev:web:linked": "GENERATE_SOURCEMAP=false BROWSER=none pnpm run dev:web",
    "dev:packages": "pnpm exec tsc --build --watch --preserveWatchOutput tsconfig.packages.json",
    "hdwallet:build": "pnpm exec tsc --build tsconfig.hdwallet.json && pnpm -r --filter=\"@shapeshiftoss/hdwallet-*\" postbuild",
    "hdwallet:sandbox": "pnpm run hdwallet:build && pnpm --filter @shapeshiftoss/hdwallet-sandbox dev",
    "hdwallet:test:integration": "pnpm --filter @shapeshiftoss/hdwallet-integration test",
    "hdwallet:test": "pnpm exec vitest run -c vitest.hdwallet.config.mts",
    "hdwallet:docker:emulator:run": "docker run -p 5000:5000 kktech/kkemu:latest",
    "hdwallet:docker:emulator:stop": "docker ps -q --filter ancestor=kktech/kkemu:latest | xargs -r docker stop",
    "dev:swap-widget": "pnpm --filter @shapeshiftoss/swap-widget dev",
    "generate:all": "pnpm run generate:caip-adapters && pnpm run generate:color-map && pnpm run generate:asset-data && pnpm run generate:tradable-asset-map && pnpm run generate:thor-longtail-tokens",
    "generate:chain": "pnpm exec tsx ./scripts/generateChainAssetData.ts",
    "generate:caip-adapters": "pnpm --filter @shapeshiftoss/caip generate",
    "generate:asset-data": "pnpm exec tsx ./scripts/generateAssetData/generateAssetData.ts && pnpm run codemod:clear-assets-migration",
    "generate:related-asset-index": "pnpm exec tsx ./scripts/generateAssetData/generateRelatedAssetIndex.ts && pnpm run codemod:clear-assets-migration",
    "generate:color-map": "pnpm exec tsx ./scripts/generateAssetData/generateColorMap.ts",
    "generate:tradable-asset-map": "pnpm exec tsx ./scripts/generateTradableAssetMap/index.ts",
    "generate:thor-longtail-tokens": "pnpm exec tsx ./scripts/generateThorLongtailTokens/generateThorLongtailTokens.ts",
    "local-ci": "pnpm run build:packages && pnpm run lint && pnpm run type-check && pnpm run test && pnpm run build:web",
    "lint": "pnpm exec eslint --cache '*/**/*.{ts,tsx}'",
    "lint:fix": "pnpm run lint --fix",
    "postinstall": "git config --local include.path '.gitconfig'",
    "release": "pnpm exec tsx ./scripts/release.ts",
    "scrape-walletconnect-dapps": "pnpm exec tsx ./scripts/scrapeWalletConnectRegistry.ts",
    "test": "pnpm run test:packages && pnpm run test:web",
    "test:web": "pnpm exec vitest run -c vitest-web.config.mts && pnpm exec vitest run -c vitest-side-effects.config.mts",
    "test:packages": "pnpm exec vitest run -c vitest-packages.config.mts",
    "translations:count-words-invalidate-since": "pnpm exec tsx ./scripts/translations/countWordsInvalidateSince.ts",
    "translations:update-path": "pnpm exec tsx ./scripts/translations/updateTranslationPath.ts",
    "type-check": "NODE_OPTIONS=--max-old-space-size=8192 pnpm exec tsc --noEmit",
    "type-check-watch": "pnpm exec tsc --noEmit --watch"
  },
  "browserslist": {
    "production": [
      "chrome >= 67",
      "edge >= 79",
      "firefox >= 68",
      "opera >= 54",
      "safari >= 14"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "fork-ts-checker": {
    "typescript": {
      "memoryLimit": 4096
    }
  },
