{
  "name": "@shapeshiftoss/investor-foxy",
  "version": "3.0.1",
  "description": "An API layer for interaction with the ShapeShift FOXy contract",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "lib": "dist",
    "src": "src"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=14.18.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "yarn clean && yarn compile",
    "clean": "rm -rf dist && rm -rf tsconfig.build.tsbuildinfo",
    "compile": "tsc -p tsconfig.build.json",
    "dev": "tsc --watch",
    "prepare": "yarn build",
    "test": "jest --coverage",
    "cli": "yarn build && node ./dist/foxycli.js"
  },
  "dependencies": {
    "@ethersproject/providers": "^5.5.3",
    "axios": "^0.26.1",
    "bignumber.js": "^9.0.2",
    "lodash": "^4.17.21",
    "web3": "^1.7.1",
    "web3-core": "^1.7.1",
    "web3-utils": "^1.7.1"
  },
  "peerDependencies": {
    "@shapeshiftoss/caip": "^4.0.0",
    "@shapeshiftoss/chain-adapters": "^4.0.0",
    "@shapeshiftoss/hdwallet-core": "^1.20.0",
    "@shapeshiftoss/types": "^4.0.0"
  },
  "devDependencies": {
    "@shapeshiftoss/caip": "^4.0.0",
    "@shapeshiftoss/chain-adapters": "^4.0.0",
    "@shapeshiftoss/hdwallet-core": "^1.20.0",
    "@shapeshiftoss/types": "^4.0.0"
  }
}
