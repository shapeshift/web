{
  "name": "@shapeshiftoss/unchained-client",
  "version": "10.14.9-coderabbit-fix.2",
  "repository": "https://github.com/shapeshift/web",
  "license": "MIT",
  "type": "module",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "pnpm run clean && pnpm run generate && pnpm exec tsc --build && pnpm run postbuild",
    "build:docker": "pnpm run clean:dist && pnpm exec tsc --build && pnpm run postbuild",
    "clean": "rm -rf dist src/generated",
    "clean:dist": "rm -rf dist",
    "dev": "pnpm exec tsc --build --watch",
    "generate": "TS_POST_PROCESS_FILE=./generator/post_process.sh JAVA_OPTS='-Dlog.level=error' openapi-generator-cli generate",
    "postbuild": "pnpm run postbuild:esm && pnpm run postbuild:cjs",
    "postbuild:esm": "pnpm exec tsc-esm-fix --target=dist/esm --ext=.js",
    "postbuild:cjs": "echo '{\"type\": \"commonjs\"}' > dist/cjs/package.json"
  },
  "dependencies": {
    "@shapeshiftoss/caip": "workspace:^",
    "@shapeshiftoss/common-api": "^9.3.0",
    "@shapeshiftoss/contracts": "workspace:^",
    "@shapeshiftoss/utils": "workspace:^",
    "axios": "^1.13.5",
    "bignumber.js": "^9.3.1",
    "ethers": "6.11.1",
    "isomorphic-ws": "^4.0.1",
    "viem": "2.43.5",
    "ws": "^8.17.1"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.5.1"
  }
}
