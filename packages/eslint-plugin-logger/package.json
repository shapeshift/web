{
  "name": "@shapeshiftoss/eslint-plugin-logger",
  "description": "An eslint plugin disallowing console.* in favor of their moduleLogger equivalent",
  "version": "1.1.0",
  "main": "dist/index.js",
  "source": "src/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
