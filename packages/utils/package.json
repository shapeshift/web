{
  "name": "@shapeshiftoss/utils",
  "version": "1.0.5",
  "repository": "https://github.com/shapeshift/web",
  "license": "MIT",
  "type": "module",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "pnpm run clean && pnpm exec tsc --build && pnpm run postbuild",
    "clean": "rm -rf dist",
    "dev": "pnpm exec tsc --build --watch",
    "postbuild": "pnpm run postbuild:esm && pnpm run postbuild:cjs",
    "postbuild:esm": "pnpm exec tsc-esm-fix --target=dist/esm --ext=.js",
    "postbuild:cjs": "echo '{\"type\": \"commonjs\"}' > dist/cjs/package.json"
  },
  "dependencies": {
    "@shapeshiftoss/caip": "workspace:^",
    "@shapeshiftoss/types": "workspace:^",
    "@sniptt/monads": "^0.5.10",
    "bignumber.js": "^9.3.1",
    "dayjs": "^1.11.3",
    "lodash-es": "^4.17.23"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.12"
  }
}
