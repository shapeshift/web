{
  "name": "@shapeshiftoss/chain-adapters",
  "version": "11.3.3",
  "repository": "https://github.com/shapeshift/web",
  "license": "MIT",
  "main": "./dist-cjs/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist-cjs/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "dist-cjs"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "yarn clean && yarn run -T tsc --build && yarn run -T tsc --build tsconfig.cjs.json",
    "build:cjs": "yarn run -T tsc --build tsconfig.cjs.json",
    "clean": "rm -rf dist dist-cjs && rm -f tsconfig*.tsbuildinfo 2>/dev/null || true",
    "dev": "yarn run -T tsc --build --watch"
  },
  "dependencies": {
    "@shapeshiftoss/caip": "workspace:^",
    "@shapeshiftoss/types": "workspace:^",
    "@shapeshiftoss/unchained-client": "workspace:^",
    "@shapeshiftoss/utils": "workspace:^",
    "@solana/spl-token": "^0.4.9",
    "@solana/web3.js": "^1.98.0",
    "bech32": "^2.0.0",
    "coinselect": "^3.1.13",
    "multicoin-address-validator": "^0.5.12",
    "p-queue": "^8.0.1"
  },
  "devDependencies": {
    "@types/multicoin-address-validator": "^0.5.0"
  }
}
