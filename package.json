{
  "name": "@shapeshiftoss/web",
  "version": "1.27.0",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*"
    ],
    "version": "independent"
  },
  "scripts": {
    "bump": "yarn upgrade-interactive --latest --scope @shapeshiftoss",
    "build:web": "yarn build:packages && NODE_OPTIONS=--max-old-space-size=8192 IMAGE_INLINE_SIZE_LIMIT=0 INLINE_RUNTIME_CHUNK=false DISABLE_ESLINT_PLUGIN=true react-app-rewired --openssl-legacy-provider build && tsx ./scripts/writeHeaders.ts && ./scripts/sha256sums.sh && ./scripts/verifyWebpackHashes.sh && tsx ./scripts/writeBuildMetadata.ts",
    "build:packages": "yarn clean:packages && yarn workspace @shapeshiftoss/unchained-client generate && yarn tsc --build tsconfig.packages.json",
    "clean": "yarn clean:packages && yarn clean:web && rm -rf node_modules coverage tsconfig.tsbuildinfo",
    "clean:web": "rm -rf build tsconfig.web.tsbuildinfo",
    "clean:packages": "yarn workspaces foreach -ptiv --exclude @shapeshiftoss/web run clean && rm -rf tsconfig.packages.tsbuildinfo",
    "codemod:clear-assets-migration": "jscodeshift -t src/state/migrations/transformClearAssets.ts --parser=ts --extensions=ts src/state/migrations/index.ts",
    "deadcode": "yarn ts-prune",
    "dev": "yarn concurrently -n \"packages,web\" \"yarn dev:packages\" \"yarn dev:web\"",
    "dev:web": "BROWSER=none NODE_OPTIONS=--max-old-space-size=4096 ESLINT_NO_DEV_ERRORS=true TSC_COMPILE_ON_ERROR=true IMAGE_INLINE_SIZE_LIMIT=0 INLINE_RUNTIME_CHUNK=false yarn react-app-rewired --openssl-legacy-provider start",
    "dev:web:linked": "GENERATE_SOURCEMAP=false BROWSER=none yarn dev:web",
    "dev:packages": "yarn tsc --build --watch --preserveWatchOutput tsconfig.packages.json",
    "env": "yarn tsx ./scripts/bootstrap-env.ts",
    "generate:caip-adapters": "yarn workspace @shapeshiftoss/caip generate",
    "generate:asset-data": "yarn tsx ./scripts/generateAssetData/generateAssetData.ts && yarn run codemod:clear-assets-migration",
    "generate:related-asset-index": "yarn tsx ./scripts/generateAssetData/generateRelatedAssetIndex.ts && yarn run codemod:clear-assets-migration",
    "generate:color-map": "yarn tsx ./scripts/generateAssetData/generateColorMap.ts",
    "generate:thor-asset-map": "yarn tsx ./scripts/generateTradableThorAssetMap/generateTradableThorAssetMap.ts",
    "generate:thor-longtail-tokens": "yarn tsx ./scripts/generateThorLongtailTokens/generateThorLongtailTokens.ts",
    "local-ci": "yarn lint && yarn type-check && yarn test && yarn build:web",
    "lint": "yarn eslint --cache '*/**/*.{ts,tsx}'",
    "lint:fix": "yarn lint --fix",
    "postinstall": "git config --local include.path '.gitconfig'",
    "release": "yarn tsx ./scripts/release.ts",
    "scrape-walletconnect-dapps": "yarn tsx ./scripts/scrapeWalletConnectRegistry.ts",
    "test": "yarn test:packages && yarn test:web",
    "test:web": "yarn vitest run -c vitest-web.config.ts && yarn vitest run -c vitest-side-effects.config.ts",
    "test:packages": "yarn vitest run -c vitest-packages.config.ts",
    "test:ci": "yarn test:packages && yarn test:web",
    "test:dev:web": "yarn react-app-rewired test --watch",
    "test:dev:packages": "yarn test:packages --watch",
    "translations:count-words-invalidate-since": "yarn tsx ./scripts/translations/countWordsInvalidateSince.ts",
    "translations:update-path": "yarn tsx ./scripts/translations/updateTranslationPath.ts",
    "type-check": "yarn tsc --noEmit",
    "type-check-watch": "yarn tsc --noEmit --watch"
  },
  "browserslist": {
    "production": [
      "chrome >= 67",
      "edge >= 79",
      "firefox >= 68",
      "opera >= 54",
      "safari >= 14"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config-overrides-path": "react-app-rewired.config.js",
  "fork-ts-checker": {
    "typescript": {
      "memoryLimit": 4096
    }
  },
