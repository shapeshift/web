{
  "name": "@shapeshiftoss/web",
  "version": "1.27.0",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*"
    ],
    "version": "independent"
  },
  "scripts": {
    "bump": "yarn upgrade-interactive --latest --scope @shapeshiftoss",
    "build": "yarn build:packages",
    "build:web": "yarn build && NODE_OPTIONS=--max-old-space-size=8192 IMAGE_INLINE_SIZE_LIMIT=0 INLINE_RUNTIME_CHUNK=false DISABLE_ESLINT_PLUGIN=true react-app-rewired build && node ./scripts/writeHeaders.js && ./scripts/sha256sums.sh && ./scripts/verifyWebpackHashes.sh && ts-node --skipProject ./scripts/writeBuildMetadata.ts",
    "build:packages": "yarn clean:packages && yarn workspace @shapeshiftoss/unchained-client generate && yarn tsc --build tsconfig.packages.json",
    "clean": "yarn clean:packages && yarn clean:web && rm -rf node_modules coverage tsconfig.tsbuildinfo",
    "clean:web": "rm -rf build tsconfig.web.tsbuildinfo",
    "clean:packages": "yarn workspaces foreach -ptiv --exclude @shapeshiftoss/web run clean && rm -rf tsconfig.packages.tsbuildinfo",
    "deadcode": "yarn ts-prune",
    "dev": "yarn concurrently -n \"packages,web\" \"yarn dev:packages\" \"yarn dev:web\"",
    "dev:web": "BROWSER=none NODE_OPTIONS=--max-old-space-size=4096 ESLINT_NO_DEV_ERRORS=true TSC_COMPILE_ON_ERROR=true IMAGE_INLINE_SIZE_LIMIT=0 INLINE_RUNTIME_CHUNK=false CYPRESS_RELAXED_SECURITY=true yarn react-app-rewired start",
    "dev:web:linked": "GENERATE_SOURCEMAP=false BROWSER=none yarn dev:web",
    "dev:packages": "yarn tsc --build --watch --preserveWatchOutput tsconfig.packages.json",
    "env": "yarn ts-node --skipProject ./scripts/bootstrap-env.ts",
    "generate:abis": "yarn typechain src/contracts/abis/*.json --target=ethers-v5 --out-dir ./src/contracts/__generated",
    "local-ci": "yarn lint && yarn type-check && yarn test && yarn build:web",
    "lint": "yarn eslint --cache '*/**/*.{ts,tsx}'",
    "lint:fix": "yarn lint --fix",
    "lint:cypress": "yarn eslint -c ./cypress/.eslintrc cypress --ext .ts,.tsx",
    "lint:cypress:fix": "yarn lint:cypress --fix",
    "postinstall": "git config --local include.path '.gitconfig'",
    "release": "yarn ts-node --skipProject ./scripts/release.ts",
    "remove-outdated-translations": "yarn ts-node --skipProject ./scripts/removeOutdatedTranslations.ts",
    "scrape-walletconnect-dapps": "yarn ts-node --skipProject ./scripts/scrapeWalletConnectRegistry.ts",
    "test": "yarn test:packages && yarn test:web",
    "test:web": "yarn jest react-app-rewired && react-app-rewired test --watchAll=false",
    "test:packages": "yarn jest packages",
    "test:ci": "yarn test:packages && yarn test:web",
    "test:cypress": "yarn cypress open",
    "test:cypress:headless": "yarn cypress run --browser chrome --headless",
    "test:cypress:rerecord": "yarn cypress run --browser chrome --headless --env forceRecord=true",
    "test:cypress:clean": "yarn cypress run --browser chrome --headless --env cleanMocks=true",
    "test:dev:web": "yarn react-app-rewired test --watch",
    "test:dev:packages": "yarn test:packages --watch",
    "type-check": "yarn tsc --noEmit",
    "type-check-watch": "yarn tsc --noEmit --watch"
  },
  "browserslist": {
    "production": [
      "chrome >= 67",
      "edge >= 79",
      "firefox >= 68",
      "opera >= 54",
      "safari >= 14"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config-overrides-path": "react-app-rewired.config.js",
  "fork-ts-checker": {
    "typescript": {
      "memoryLimit": 4096
    }
  },
