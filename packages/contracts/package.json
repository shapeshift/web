{
  "name": "@shapeshiftoss/contracts",
  "version": "1.0.5-coderabbit-fix.2",
  "repository": "https://github.com/shapeshift/web",
  "license": "MIT",
  "type": "module",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "pnpm run clean && pnpm exec tsc --build && pnpm run postbuild",
    "clean": "rm -rf dist",
    "dev": "pnpm exec tsc --build --watch",
    "postbuild": "pnpm run postbuild:esm && pnpm run postbuild:cjs",
    "postbuild:esm": "pnpm exec tsc-esm-fix --target=dist/esm --ext=.js",
    "postbuild:cjs": "echo '{\"type\": \"commonjs\"}' > dist/cjs/package.json"
  },
  "dependencies": {
    "@shapeshiftoss/caip": "workspace:^",
    "@shapeshiftoss/types": "workspace:^",
    "@shapeshiftoss/utils": "workspace:^",
    "@uniswap/sdk": "^3.0.3",
    "ethers": "6.11.1",
    "ethers5": "npm:ethers@5.7.2",
    "lodash": "^4.17.23",
    "viem": "2.43.5"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.178"
  }
}
