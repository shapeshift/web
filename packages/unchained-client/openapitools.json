{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "6.1.0",
    "storageDir": "~/.openapitools",
    "generators": {
      "avalanche": {
        "inputSpec": "https://raw.githubusercontent.com/shapeshift/unchained/develop/node/coinstacks/avalanche/api/src/swagger.json",
        "generatorName": "typescript-fetch",
        "output": "#{cwd}/src/generated/avalanche",
        "enablePostProcessFile": true,
        "reservedWordsMappings": {
          "in": "in"
        },
        "additionalProperties": {
          "supportsES6": "true",
          "useSingleRequestParameter": true
        }
      },
