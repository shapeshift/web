{
  "name": "@shapeshiftoss/hdwallet-metamask-multichain",
  "version": "1.62.41",
  "license": "MIT",
  "type": "module",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "pnpm run clean && pnpm exec tsc --build && pnpm run postbuild",
    "clean": "rm -rf dist",
    "dev": "pnpm exec tsc --build --watch",
    "postbuild": "pnpm run postbuild:esm && pnpm run postbuild:cjs",
    "postbuild:esm": "pnpm exec tsc-esm-fix --target=dist/esm --ext=.js",
    "postbuild:cjs": "echo '{\"type\": \"commonjs\"}' > dist/cjs/package.json"
  },
  "dependencies": {
    "@metamask/detect-provider": "^1.2.0",
    "@metamask/onboarding": "^1.0.1",
    "@shapeshiftoss/common-api": "^9.3.0",
    "@shapeshiftoss/hdwallet-core": "workspace:^",
    "@shapeshiftoss/metamask-snaps-adapter": "^1.0.12",
    "@shapeshiftoss/metamask-snaps-types": "^1.0.12",
    "eth-rpc-errors": "^4.0.3",
    "ethers": "5.7.2",
    "lodash": "^4.17.23",
    "mipd": "^0.0.7"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/lodash": "^4.14.178"
  },
  "sideEffects": false
}
