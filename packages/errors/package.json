{
  "name": "@shapeshiftoss/errors",
  "version": "1.1.5",
  "description": "Common set of typed errors",
  "repository": "https://github.com/shapeshift/web",
  "type": "module",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "pnpm run clean && pnpm exec tsc --build && pnpm run postbuild",
    "clean": "rm -rf dist",
    "dev": "pnpm exec tsc --build --watch",
    "postbuild": "pnpm run postbuild:esm && pnpm run postbuild:cjs",
    "postbuild:esm": "pnpm exec tsc-esm-fix --target=dist/esm --ext=.js",
    "postbuild:cjs": "echo '{\"type\": \"commonjs\"}' > dist/cjs/package.json"
  },
  "dependencies": {
    "lodash.clonedeep": "^4.5.0",
    "lodash.snakecase": "^4.1.1"
  },
  "devDependencies": {
    "@types/lodash.clonedeep": "^4.5.7",
    "@types/lodash.snakecase": "^4.1.7"
  }
}
